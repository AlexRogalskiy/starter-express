{
  "name": "remix-starter-express",
  "version": "1.0.0",
  "scripts": {
    "build": "remix build",
    "dev": "concurrently \"remix run\" \"cross-env NODE_ENV=development nodemon -w app -w build -e js,jsx,ts,tsx,md,mdx server.js\"",
    "start": "node server.js"
  },
  "dependencies": {
    "@remix-run/cli": "^0.8.0",
    "@remix-run/data": "^0.8.0",
    "@remix-run/express": "^0.8.0",
    "@remix-run/react": "^0.8.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "morgan": "^1.10.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.5"
  },
  "engines": {
    "node": "12"
  }
}
